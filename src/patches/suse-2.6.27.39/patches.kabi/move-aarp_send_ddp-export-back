+++ b/net/appletalk/aarp.c
@@ -676,7 +676,6 @@ free_it:
 drop:
 	return NET_XMIT_DROP;
 }
-EXPORT_SYMBOL(aarp_send_ddp);
 
 /*
  *	An entry in the aarp unresolved queue has become resolved. Send
